

.wrapper
  display: grid
  grid-template-columns: 50px 1fr 1fr 1fr 50px
  grid-template-rows: repeat(5, 1fr)

.header
  // background-image: url("https://images.pexels.com/photos/248797/pexels-photo-248797.jpeg?auto=compress&cs=tinysrgb&h=350")
  background-color: coral
  grid-column-start: 1
  grid-column-end: -1
  grid-row-start: 1

#my_name
  grid-column-start: 2
  grid-column-end: 4
  grid-row-start: 1

.projects_section
  // display: grid
  background-color: aqua 
  grid-column-start: 2
  grid-column-end: -2
  grid-row-start: 2
  grid-row-end: 3
  // justify-self: center
  border: 4px solid black
  padding: 50px

  .projects_section ul
    margin: 50px

