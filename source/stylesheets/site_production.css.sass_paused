.about
  background-color: yellow   
  grid-column-start: 2
  grid-column-end: -3
  grid-row-start: 3
  grid-row-end: 5
  padding: 30px

#my_image
  background-image: url("https://images.pexels.com/photos/220453/pexels-photo-220453.jpeg?auto=compress&cs=tinysrgb&h=350")
  background-repeat: no-repeat
  background-size: 100px
  background-position: center
  grid-column-start: 4
  grid-column-end: 5
  grid-row-start: 3
  grid-row-end: 4

#my_social_media

.contact
  background-color: pink
  grid-column-start: 2
  grid-column-end: -2

